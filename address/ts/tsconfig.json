{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "moduleResolution": "NodeNext",
      "lib": ["ES2020"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "types": ["node"],
      "outDir": "dist"
    },
    "include": ["*.ts"],
    "exclude": ["dist", "node_modules"]
  }